<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IPlayerServices" />
                <binding name="BasicHttpBinding_IWordServices" />
                <binding name="BasicHttpBinding_IGameServices" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:56216/PlayerServices.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IPlayerServices"
                contract="PlayerServices.IPlayerServices" name="BasicHttpBinding_IPlayerServices" />
            <endpoint address="http://localhost:56216/WordServices.svc" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IWordServices" contract="WordServices.IWordServices"
                name="BasicHttpBinding_IWordServices" />
            <endpoint address="http://localhost:56216/GameServices.svc" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IGameServices" contract="GameServices.IGameServices"
                name="BasicHttpBinding_IGameServices" />
        </client>
    </system.serviceModel>
</configuration>